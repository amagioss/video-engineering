{
  "name": "html5-to-video",
  "version": "2.0.0",
  "description": "Convert HTML5 files to ARGB MOV video format using Puppeteer with alpha channel support",
  "main": "dist/index.js",
  "bin": {
    "html5-to-video": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "prepare": "npm run build"
  },
  "keywords": ["html5", "video", "conversion", "puppeteer", "mov", "argb", "alpha-channel"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "puppeteer": "^21.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.2.0"
  },
  "optionalDependencies": {
    "sharp": "^0.33.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
} 